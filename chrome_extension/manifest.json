{
  "manifest_version": 2,

  "name": "Dynamic Menu Chrome Plugin",
  "description": "Dinamic Menu from Python.",
  "version": "1.0",

  "browser_action": {
    "default_popup": "index.html"
  },
  "permissions": [
    "activeTab",
    "tabs",
    "https://ajax.googleapis.com/",
    "http://127.0.0.1:8000/api/"
  ]
}
